    "N = int(input())\n",
    "str1 = []\n",
    "\n",
    "for i in range(N):\n",
    "  str2 = input()\n",
    "  strLen = len(str2)\n",
    "  str1.append((str2,strLen))\n",
    "  \n",
    "\n",
    "\n",
    "# 중복제거 해야함\n",
    "str3 = set(str1)\n",
    "\n",
    "str_sorted = sorted(str3, key=lambda str3 :(str3[1], str3[0]))\n",
    "\n",
    "for i in str_sorted :\n",
    "  print(i[0])"
   ]
  }
