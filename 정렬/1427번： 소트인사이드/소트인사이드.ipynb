    "import sys\n",
    "def input() :\n",
    "  return sys.stdin.readline()\n",
    "\n",
    "N = int(input())\n",
    "nums = []\n",
    "\n",
    "while N > 0 :\n",
    "  num = N % 10\n",
    "  nums.append(num)\n",
    "  N = N // 10\n",
    "  \n",
    "nums.sort(reverse=True)\n",
    "for i in nums :\n",
    "  print(i, end='')"
   ]
