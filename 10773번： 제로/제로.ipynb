    "import sys\n",
    "def input() :\n",
    "  return sys.stdin.readline()\n",
    "\n",
    "K = int(input())\n",
    "Knums = []\n",
    "cnt = 0\n",
    "for i in range(K) :\n",
    "  num = int(input())\n",
    "  if num != 0 :\n",
    "    Knums.append(num)\n",
    "  else :\n",
    "    Knums.pop()\n",
    "if len(Knums) > 0 :\n",
    "  for i in Knums :\n",
    "    cnt += i\n",
    "print(cnt)"
   ]
