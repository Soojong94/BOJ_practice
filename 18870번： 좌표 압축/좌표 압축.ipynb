    "N = int(input())\n",
    "X = list(map(int, input().split()))\n",
    "\n",
    "X_sorted_ = sorted(list(set(X)))\n",
    "\n",
    "index_dict = {X_sorted_[i]: i for i in range(len(X_sorted_))}\n",
    "\n",
    "# 인덱스를 차례로 출력\n",
    "for i in X:\n",
    "    print(index_dict[i], end=' ')\n"
   ]
  }
