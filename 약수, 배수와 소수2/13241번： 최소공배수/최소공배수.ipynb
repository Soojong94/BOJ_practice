    "\n",
    "def lcm (num1, num2) :\n",
    "  return (num1 * num2) // math.gcd(num1, num2)\n",
    "\n",
    "A, B = map(int, input().split())\n",
    "multi = lcm(A, B)\n",
    "\n",
    "print(multi)"
