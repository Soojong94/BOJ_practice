    "# 입력 받는다\n",
    "# 문자열을 각각 나눈다\n",
    "S_char = list(input())\n",
    "\n",
    "# 각각 붙인다\n",
    "#슬라이싱 활용\n",
    "S_list = []\n",
    "for i in range(len(S_char)):\n",
    "    for j in range(i + 1, len(S_char) + 1):\n",
    "        S_list.append(''.join(S_char[i:j]))\n",
    "\n",
    "# set으로 중복 제거하고 len 을 출력\n",
    "S_seted = set(S_list)\n",
    "print(len(S_seted))\n",
    "\n"
   ]
