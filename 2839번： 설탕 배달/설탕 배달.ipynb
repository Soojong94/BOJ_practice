   "source": [
    "N = int(input())\n",
    "cnt = 0\n",
    "\n",
    "\n",
    "while N >= 0 :\n",
    "  if N % 5 == 0 :\n",
    "    cnt = cnt + N // 5\n",
    "    break\n",
    "  elif N % 5 != 0 :\n",
    "    N -= 3 \n",
    "    cnt += 1\n",
    "    if N < 0 :\n",
    "      cnt = -1\n",
    "      \n",
    "print(cnt)"
   ]
  }
