    "import sys\n",
    "def input() :\n",
    "  return sys.stdin.readline()\n",
    "from collections import deque\n",
    "\n",
    "N = int(input())\n",
    "que = deque()\n",
    "que.extend([i for i in range(1,N+1)])\n",
    "\n",
    "while len(que) > 1 :\n",
    "  que.popleft()\n",
    "  que.append(que.popleft())\n",
    "  \n",
    "print(que[0])"
   ]
